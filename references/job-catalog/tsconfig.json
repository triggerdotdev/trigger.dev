      "@trigger.dev/core": [
        "../../packages/core/src/index"
      ],
      "@trigger.dev/core/*": [
        "../../packages/core/src/*"
      ],
      "@trigger.dev/integration-kit": [
        "../../packages/integration-kit/src/index"
      ],
      "@trigger.dev/integration-kit/*": [
        "../../packages/integration-kit/src/*"
      ],
      "@trigger.dev/github": [
        "../../integrations/github/src/index"
      ],
      "@trigger.dev/github/*": [
        "../../integrations/github/src/*"
      ],
      "@trigger.dev/slack": [
        "../../integrations/slack/src/index"
      ],
      "@trigger.dev/slack/*": [
