      ],
      "@trigger.dev/sdk": [
        "../../packages/trigger-sdk/src/index"
      ],
      "@trigger.dev/sdk/*": [
        "../../packages/trigger-sdk/src/*"
      ],
      "@trigger.dev/express": [
        "../../packages/express/src/index"
