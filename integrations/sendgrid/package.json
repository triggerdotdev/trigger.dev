{
  "name": "@trigger.dev/sendgrid",
  "version": "3.0.0-beta.43",
  "description": "Trigger.dev integration for @sendgrid/mail",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@trigger.dev/tsconfig": "workspace:*",
    "@types/node": "16.x",
    "rimraf": "^3.0.2",
    "tsup": "8.0.1",
    "typescript": "^5.3.0",
    "@trigger.dev/tsup": "workspace:*"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@trigger.dev/sdk": "workspace:^3.0.0-beta.43",
    "@trigger.dev/integration-kit": "workspace:^3.0.0-beta.43"
  },
  "engines": {
    "node": ">=16.8.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.mjs"
}
