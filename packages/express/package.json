{
  "name": "@trigger.dev/express",
  "version": "2.1.3",
  "description": "Official Express adapter for Trigger.dev",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@trigger.dev/sdk": "workspace:^2.1.3",
    "@trigger.dev/tsconfig": "workspace:*",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "rimraf": "^3.0.2",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup"
  },
  "peerDependencies": {
    "@trigger.dev/sdk": "workspace:^2.1.3"
  },
  "dependencies": {
    "@remix-run/web-fetch": "^4.3.5",
    "debug": "^4.3.4",
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=16.8.0"
  }
}