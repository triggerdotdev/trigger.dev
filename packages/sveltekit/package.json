{
  "name": "@trigger.dev/sveltekit",
  "version": "0.0.1",
  "description": "Trigger.dev svelteKit integration",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
